    <body class="default-themes themes-tetholiday homepage">
        <div class="overlay-light"></div>
        <nav class="navbar navbar-default navbar-fixed-top navbar-shrink menu-z-index" style="z-index: 2005;">
            <div class="header">
                <div class="container">
                    @Display(WorkContext.Layout.HomeLogo)
                        
                    @Html.Partial("BoxLogin")  

                    @Html.Partial("HomeNavigatorMenu")
                        
                    @Html.Partial("SearchBox")
                </div>
            </div>
        </nav>
        
        @Display(WorkContext.Layout.AdBannerPageLeftVip) 
        @Display(WorkContext.Layout.AdBannerPageLeft)
        
        <div class="container">
            <div id="homeBanner" class="banner_add-full">
                <div class="banner_add">
                    <div style="z-index: 2002; width: 1000px; margin: auto; position: relative;">
                        @Display(WorkContext.Layout.SliderBanner)
                    </div>
                    <div class="clear_both"></div>
                    @Display(WorkContext.Layout.AdBannerPageCenter) 
                    @Display(WorkContext.Layout.AdBannerPageCenterVip)  
                </div> 
            </div>
        </div>

        @Display(WorkContext.Layout.AdBannerPageRight) 
        @Display(WorkContext.Layout.AdBannerPageRightVip)
        
        <div class="clear"></div>
        
        <div class="container">
            <div class="content_div">
                @Display(WorkContext.Layout.FullContent)  
                <div class="content_left">
                    @Display(WorkContext.Layout.Content)  
                                    
                    @Display(WorkContext.Layout.CategoryContentLeftFirst)  
                                                                   
                    @Display(WorkContext.Layout.AdBannerContentLeftFirst)  
                                                              
                    @Display(WorkContext.Layout.CategoryContentLeftSecond)  
                                                
                    @Display(WorkContext.Layout.AdBannerContentLeftSecond)  
		                             
                    @Display(WorkContext.Layout.CategoryContentLeftThird)  
                                       
                    @Display(WorkContext.Layout.AdBannerContentLeftThird)  
   
                    @Display(WorkContext.Layout.CategoryContentLeftFourth) 
                                   
                    @Display(WorkContext.Layout.AdBannerContentLeftFourth)

                    @Display(WorkContext.Layout.CategoryContentLeftFifth) 
                                        
                    @Display(WorkContext.Layout.AdBannerContentLeftFifth)

                    @Display(WorkContext.Layout.CategoryContentLeftSixth) 
                     
                    @Display(WorkContext.Layout.AdBannerContentLeftSixth)

                    @Display(WorkContext.Layout.CategoryContentLeftSeventh) 
                                                                   
                    @Display(WorkContext.Layout.AdBannerContentLeftSeventh)
                </div>  

                <div class="content_right">
                    @Display(WorkContext.Layout.AdBannerRightFree) 
                         
                    @Display(WorkContext.Layout.AdBannerRightFirst)           
                                  
                    @Display(WorkContext.Layout.AdFacebookFanpage)
                                      
                    @Display(WorkContext.Layout.DisplayNews)     
                                
                    @Display(WorkContext.Layout.AdBannerRightFifth)

                    @Display(WorkContext.Layout.DisplayStatistic1)
                                                              
                    @Display(WorkContext.Layout.DisplayStatistic2)
                                   
                    @Display(WorkContext.Layout.AdBannerRightSixth) 
                                                                
                    @Display(WorkContext.Layout.AdBannerRightSecond) 
                                                                       
                    @Display(WorkContext.Layout.AdBannerRightThird) 
                                
                    @Display(WorkContext.Layout.DisplayTags)

                    @Display(WorkContext.Layout.AdBannerRightFourth)   
                    
                    @Display(WorkContext.Layout.DisplaySocialNetwork)    
                </div> 
            </div>     
                       
            @Html.Partial("HomeLogin")
                    
            @Html.Partial("Register")     
                    
            @Html.Partial("ForgetPassword")

            @Html.Partial("HomeShowDialog") 
                    
            @Display(WorkContext.Layout.RegisterResource) 
        </div>
        
        <div class="clear_both"></div>

        <div class="footer">
            <div class="footer_960">
                @Html.Partial("HomeFooterMenu")             

                @Display(WorkContext.Layout.FooterLogoInformation) 
            </div>   
            <div class="clear"></div>   
        </div> 

        @RenderFootScripts()    
        
       <script type="text/javascript">
           $window = $(window);
           $bannerleft = $('.banner_add-left');
           $bannerright = $('.banner_add-right');
           $banner = $('.banner-size');
           $wiBanner = $banner.outerWidth() * 2;
           $(document).ready(function () {
               zindex($('.banner_add').outerWidth());
           });
           $(window).bind('resize', function () {
               zindex($('.banner_add').outerWidth());
           });
           function zindex(maxWidth) {
               var fullSize = (maxWidth + $wiBanner + 3);
               if ($window.width() < fullSize) {
                   var total = fullSize - $window.width();
                   var width = (total / 2);
                   //$banner.css("width", width + "px");
                   $bannerleft.css("margin-left", "-" + width + "px");
                   $bannerright.css("margin-right", "-" + width + "px");
               } else {
                   //$banner.css("width", "173px");
                   $bannerleft.css("margin-left", "auto");
                   $bannerright.css("margin-right", "auto");
               }
           }
        </script>
    </body>
</html>
