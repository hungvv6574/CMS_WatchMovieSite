        <meta name="copyright" content="2015">
        <link rel="shortcut icon" href="/Images/favicon.ico" type="image/x-icon" />   
        <meta http-equiv="Content-Language" content="vi"/>
		<meta name="Language" content="vietnamese"/>
        <meta name="admicro-site-verification" content="c677a845a20648b48b042c61150b8e71" />
        <meta name="google-site-verification" content="1vWUKYx8fp7tHWMLqsOxzQTih7hDvWExfUH7y0k-sfI" />
        <meta name="eclick_verify" content="dFNbW1IXLRcYGVlEahUFGQsPaxUC"/>

        <title>@ViewData[CMSSolutions.Websites.Extensions.Constants.HeaderTitle]</title>

        @RenderMetas()
        @RenderStyles()
        @RenderHeadScripts()   
         
        <script>
